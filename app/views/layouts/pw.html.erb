<meta content="" name="description"/>
<meta content="" name="author"/>
<%= stylesheet_link_tag 'application', media: 'all' %>
<%= yield(:stylesheets) %>
<link href="<%= asset_path('pages/css/pages-icons.css') %>" rel="stylesheet" type="text/css">
<link class="main-stylesheet" href="<%= asset_path('pages/css/pages.css') %>" rel="stylesheet" type="text/css"/>
